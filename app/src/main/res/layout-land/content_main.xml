<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent" android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin" android:paddingRight="@dimen/activity_horizontal_margin" android:paddingTop="@dimen/activity_vertical_margin" app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="package com.example.greg.MainActivity" tools:showIn="@layout/activity_main"
    android:background="@drawable/sport_land">

    <!--
    <TextView
    android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="Hello World!" />
    -->

    <FrameLayout
    android:layout_width="match_parent" android:layout_height="match_parent"
        android:id="@+id/myFrame2"
        android:layout_alignParentTop="true">

        <ImageButton
            android:layout_width="130dp"
            android:layout_height="110dp"
            android:id="@+id/mapButton"
            android:layout_row="0"
            android:layout_column="0"
            android:src="@drawable/pin"
            android:adjustViewBounds="false"
            android:background="#81ffffff"
            android:layout_rowWeight="1"
            android:layout_rowSpan="1"
            android:layout_columnWeight="0"
            android:layout_gravity="left"
            android:layout_marginTop="100dp"
            android:layout_marginLeft="5dp" />

        <ImageButton
            android:layout_width="130dp"
            android:layout_height="110dp"
            android:id="@+id/statButton"
            android:layout_gravity="right|top"
            android:src="@drawable/arrow"
            android:layout_marginTop="100dp"
            android:layout_marginRight="5dp"
            android:background="#81ffffff" />

        <ImageButton
            android:layout_width="130dp"
            android:layout_height="110dp"
            android:id="@+id/exchangeButton"
            android:layout_gravity="right"
            android:src="@drawable/arrows"
            android:layout_marginRight="145dp"
            android:background="#81ffffff"
            android:layout_marginTop="100dp" />

        <ImageButton
            android:layout_width="130dp"
            android:layout_height="110dp"
            android:id="@+id/messageButton"
            android:src="@drawable/black"
            android:background="#81ffffff"
            android:layout_marginBottom="50dp"
            android:layout_marginLeft="145dp"
            android:layout_marginTop="100dp" />
    </FrameLayout> </RelativeLayout>

